{
    "name": "revosystems/como-laravel-sdk",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Revo\\ComoSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Revo\\ComoSdk\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Mart√≠ Sala Morral",
            "email": "marti.sala@revo.works"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0|^9.0",
        "guzzlehttp/guzzle": "^6.0|^7.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.3",
        "orchestra/testbench": "^7.18"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
